            <script>
                function emptyJobSection() {
                    let technology = document.getElementById('search_tec').value;
                    let locatoin = document.getElementById('search_loc').value;

                    // 1. create request object
                    let obj = new XMLHttpRequest();


                    if (technology !== "" || locatoin !== "") {
                        document.getElementById('on_click_empty').innerHTML = "";
                        // 2. specify the request
                        obj.open("POST", "search_jobs?technology=" + technology + "&location=" + locatoin, true);

                        // 3 Send the request to the server
                        obj.send();

                        // 4. call the function when ready state property changes
                        obj.onreadystatechange = function () {
                            if (obj.readyState === 4 && obj.status === 200) {
                                if (obj.responseText === "") {
                                    document.getElementById('on_click_empty').innerHTML = "<div style='text-align: center; padding: 30px 0;'><h2 style='color: gray;'>NO JOB FOUND</h2><div>";
                                } else {
                                    document.getElementById('on_click_empty').innerHTML = obj.responseText;
                                }
                            }
                        };
                    } else {
                        location.reload();
                    }
                }

                function quickSearch(element_id) {
                    let technology = document.getElementById(element_id).innerHTML;
                    document.getElementById('on_click_empty').innerHTML = "";
                    document.getElementById('search_tec').value = technology;

                    // 1. create request object
                    let obj = new XMLHttpRequest();

                    // 2. specify the request
                    obj.open("POST", "search_jobs?technology=" + technology + "&location=" + "", true);

                    // 3. send the request to the server
                    obj.send();

                    // 4. call the function when ready state property changes
                    obj.onreadystatechange = function () {
                        if (obj.readyState === 4 && obj.status === 200) {
                            if (obj.responseText === "") {
                                document.getElementById('on_click_empty').innerHTML = "<div style='text-align: center; padding: 30px 0;'><h2 style='color: gray;'>NO JOB FOUND</h2><div>";
                            } else {
                                document.getElementById('on_click_empty').innerHTML = obj.responseText;
                            }
                        }
                    };


                }
            </script>
